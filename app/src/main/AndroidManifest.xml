<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.serialport">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".funtionpage.SerialPortApplication"
        android:theme="@style/AppTheme">
        <activity
            android:name=".funtionpage.MainMenu"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".funtionpage.SerialPortPreferences"></activity>
        <activity android:name=".funtionpage.ConsoleActivity"></activity>
        <activity android:name=".funtionpage.LoopbackActivity"></activity>
        <activity android:name=".funtionpage.Sending01010101Activity"></activity>

    </application>

    <uses-sdk android:minSdkVersion="3" />


</manifest>